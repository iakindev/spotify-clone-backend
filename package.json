{
  "name": "spotify-clone-server",
  "version": "1.0.0",
  "description": "Spotify Clone App Back-end",
  "main": "app.js",
  "scripts": {
    "start": "yarn build && yarn serve",
    "build": "yarn run babel src -d dist --copy-files",
    "dev": "nodemon --exec babel-node src/app.js",
    "serve": "NODE_ENV=production node dist/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "yarn run lint:ts",
    "lint:js": "tsc && yarn lint:eslint",
    "lint:eslint": "eslint './src/**/*.js'",
    "format:ts": "prettier --write './src/**/*.js' && yarn lint:eslint --fix"
  },
  "keywords": [],
  "author": "İlyas Akın <ashnwor@gmail.com>",
  "license": "GPL-3.0",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.3",
    "helmet": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "marked": "^2.0.0",
    "mongoose": "^5.9.28",
    "morgan": "^1.10.0",
    "node": "^15.8.0",
    "uuid": "^8.3.1",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "@atolye15/eslint-config": "^1.1.1",
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "eslint": "^7.6.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "typescript": "^4.1.5"
  }
}
